export PATH="$HOME/.rbenv/shims:$HOME/.rbenv/bin:/usr/local/bin:/usr/local/sbin:$HOME/Development/dotfiles/bin:/usr/local/share/npm/bin:$PATH:$HOME/.bin"

# Update package config
